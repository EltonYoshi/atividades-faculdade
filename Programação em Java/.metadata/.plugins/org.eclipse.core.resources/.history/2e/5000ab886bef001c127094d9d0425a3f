package app;

public class EventoPublico extends Evento{

	public EventoPublico() {
		super("público");
		
	}
	
	
	@SuppressWarnings("deprecation")
	@Override
	public String toString() {
		return "Nome do Evento: " + getNome() + "\nEndereço: " + getEndereco() + "\nCategoria: " + getCategoria() + "\nData: " + getData() + "\nHorário de início: " + getHorarioInicio().getHours() + 
				" : " + getHorarioInicio().getMinutes() + "\nHorário de saída: "+ getHorarioTermino().getHours() + " : " + getHorarioTermino().getMinutes()
				+ "\nDescrição: " + getDescricao() + "\nUsuarios participando: "+ getEventoUsuario();
	}
	
}
