package app;

public class EventoPrivado extends Evento {

	public EventoPrivado() {
		super("privado");
		
	}
	
	
	@Override
	public String toString() {
		return "Nome do Evento: " + getNome() + "\nEndereço: " + getEndereco() + "\nCategoria: " + getCategoria() + "\nHorário de início: " + getHorarioInicio().getHours() + 
				" : " + getHorarioInicio().getMinutes() + "\nHorário de saída: "+ getHorarioTermino().getHours() + " : " + getHorarioTermino().getMinutes()
				+ "\nDescrição: " + getDescricao() + "\nUsuarios participando: "+ getEventoUsuario();
	}
	


}
