package app;

public class Medicamento {
	private String nome;
	private String cnpj;
	private String laboratorio;
	private int codigo;
	
	
	
	public Medicamento(String nome, String cnpj, String laboratorio, int codigo) {
		this.nome = nome;
		this.cnpj= cnpj;
		this.laboratorio = laboratorio;
		this.codigo = codigo;
	}
	
	public String getNome() {
		return nome;
	}

	
	public String getCnpj() {
		return cnpj;
	}
	
	
	public String getLaboratorio() {
		return laboratorio;
	}


	public int getCodigo() {
		return codigo;
	}


	@Override
	public String toString() {
		return "Nome do Medicamento: " + this.nome + " CNPJ: " + this.cnpj + " Laboratório: " + this.laboratorio;
	}
	
	
	
	
}
